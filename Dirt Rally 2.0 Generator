def conditions(weather):
    if weather == 1:
        return 'daytime/dry'
    if weather == 2:
        return 'daytime/wet'
    if weather == 3:
        return 'sunset/dry'
    if weather == 4:
        return 'sunset/wet'
    if weather == 5:
        return 'night/dry'
    if weather == 6:
        return 'night/wet'

def carCategory(car):
    if car == 1:
        return 'Historic Rally H1 (FWD).'
    if car == 2:
        return 'Historic Rally H2 (FWD).'
    if car == 3:
        return 'Historic Rally H2 (RWD).'
    if car == 4:
        return 'F2 Kit Car.'
    if car == 5:
        return 'Historic Rally H3 (RWD).'
    if car == 6:
        return 'Historic Rally Group B (4WD).'
    if car == 7:
        return 'Historic Rally Group B (RWD).'
    if car == 8:
        return 'Modern Rally Group A.'
    if car == 9:
        return 'Modern Rally GT.'
    if car == 10:
        return 'Modern Rally R2.'
    if car == 11:
        return 'Modern Rally NR4/R4.'
    if car == 12:
        return '2000cc (4WD).'
    if car == 13:
        return 'Modern Rally R5 (4WD).'

    

r = random.randint(1, 156)
selectedStage = stage(r)

r = random.randint(1, 6)
selectedTimeOfDay = conditions(r)

r = random.randint(1, 13)
selectedCarClass = carCategory(r)

print('Drive ' + selectedStage + ' during ' + selectedTimeOfDay + ' conditions in ' + selectedCarClass)

input('Press ENTER to exit')
